* ===================================================================== *
* -----------------     	  Computing averages      ----------------- *
* ===================================================================== *

	**1. Attendance
	local attendance att_may15_16 att_july15_16 att_aug15_16 att_oct15_16 att_nov15_16 ///
		att_jan15_16 att_feb15_16 att_may14_15 att_july14_15 att_aug14_15 att_oct14_15 ///
		att_nov14_15 att_jan14_15 att_feb14_15

	cap drop check
	egen att_average=rowtotal(`attendance'), mi
	egen check=rownonmiss(`attendance')

	tab check
	qui sum check
	replace att_average=. if check<`r(max)'   
	replace att_average=att_average/`r(max)'
	drop check
	label var att_average "Average attendance over 14 months"


	**2. Subject averages
	local subject eng mat sc ss 

	foreach x in `subject'{	
	local `x' `x'_class8_sept15_16 `x'_class8_oct15_16 `x'_class8_may15_16 `x'_class8_july15_16 `x'_class8_aug15_16 ///
			`x'_class8_march14_15 `x'_class8_jan14_15 `x'_class8_feb14_15

	egen `x'_average=rowtotal(``x''), mi
	egen check=rownonmiss(``x'')

	tab check
	qui sum check
	replace `x'_average=. if check<`r(max)'   
	replace `x'_average=`x'_average/`r(max)'
	drop check
	}

	
	local hindi hindi_class8_sept15_16 hindi_class8_oct15_16 hindi_class8_may15_16 hindi_class8_july15_16 hindi_class8_aug15_16 ///
			hindi_class8_jan14_15 hindi_class8_feb14_15 

	egen hindi_average=rowtotal(`hindi'), mi
	egen check=rownonmiss(`hindi')

	tab check
	qui sum check
	replace hindi_average=. if check<`r(max)'   
	replace hindi_average=hindi_average/`r(max)'
	drop check


